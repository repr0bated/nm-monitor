{
  "version": 1,
  "plugins": {
    "netcfg": {
      "ovs_flows": [
        {
          "bridge": "ovsbr0",
          "priority": 200,
          "match_rule": "ip,nw_dst=10.0.0.0/8",
          "actions": "normal"
        },
        {
          "bridge": "ovsbr0",
          "priority": 300,
          "match_rule": "tcp,tp_dst=443",
          "actions": "output:1"
        },
        {
          "bridge": "ovsbr0",
          "priority": 250,
          "match_rule": "ip,nw_src=192.168.1.100",
          "actions": "drop"
        }
      ]
    }
  }
}

{
  "version": 1,
  "plugins": {
    "netcfg": {
      "routing": [
        {
          "destination": "10.0.0.0/8",
          "gateway": "172.16.0.1",
          "interface": "ovsbr0"
        },
        {
          "destination": "192.168.100.0/24",
          "gateway": "172.16.0.254"
        }
      ],
      "ovs_flows": [
        {
          "bridge": "ovsbr0",
          "priority": 200,
          "match_rule": "ip,nw_dst=10.0.0.0/8",
          "actions": "normal"
        },
        {
          "bridge": "ovsbr0",
          "priority": 300,
          "match_rule": "tcp,tp_dst=443",
          "actions": "output:1"
        }
      ],
      "dns": {
        "hostname": "netmaker-server",
        "search_domains": [
          "vpn.internal",
          "internal.example.com"
        ]
      }
    }
  }
}
